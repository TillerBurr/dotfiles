{
    "terminal.explorerKind": "external",
    // "terminal.integrated.shell.windows": "C:\\Windows\\sysnative\\bash.exe",
    "editor.fontFamily": " 'CaskaydiaCove NF','Delugia Nerd Font', 'FuraCode Nerd Font','CascadiaCode NF','Cascadia Code',  Consolas, 'Courier New', monospace",
    // "terminal.integrated.shell.windows": "C:\\Windows\\System32\\wsl.exe",
    "terminal.external.windowsExec": "C:\\Users\\tbaur\\AppData\\Local\\terminus\\Terminus.exe",
    "breadcrumbs.enabled": true,
    "workbench.iconTheme": "material-icon-theme",
    "editor.renderWhitespace": "none",
    "[python]": {},
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "scm.alwaysShowProviders": true,
    "editor.quickSuggestions": {
        "other": true,
        "comments": false,
        "strings": true
    },
    "git.autofetch": true,
    "nginx-conf-hint.syntax": "sublime",
    "githubNotificationsBell.oauthToken": "369f8e5ca522cec6f9a9f1c9621f14d768c72bb4",
    "githubPullRequests.showInSCM": true,
    "github.username": "baurt",
    "editor.fontLigatures": true,
    "files.exclude": {
        "**/__pycache__": true
    },
    "python.venvPath": "/.venv",
    "python.venvFolders": [
        "envs",
        ".pyenv",
        ".direnv",
        ".venv",
        "web"
    ],
    // "terminal.integrated.shell.windows": "C://Users//tbaur//AppData//Local//Microsoft//WindowsApps//53621FSApps.FluentTerminal_87x1pks76srcp//flute.exe",
    "python.autoComplete.addBrackets": true,
    "editor.tabCompletion": "on",
    "python.dataScience.sendSelectionToInteractiveWindow": true,
    "python.jediEnabled": false,
    "git.confirmSync": false,
    "autoDocstring.docstringFormat": "google",
    "autoDocstring.includeName": true,
    "editor.formatOnSave": true,
    "githubPullRequests.defaultMergeMethod": "squash",
    "python.linting.flake8Enabled": true,
    "terminal.integrated.rightClickBehavior": "default",
    "terminal.integrated.shell.windows": "powershell.exe",
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "autoDocstring.startOnNewLine": true,
    "terminal.integrated.shell.linux": "/usr/bin/zsh",
    "files.trimTrailingWhitespace": true,
    "editor.acceptSuggestionOnEnter": "smart",
    "files.autoSave": "onWindowChange",
    "diffEditor.ignoreTrimWhitespace": false,
    // "files.associations": {
    //     "*.html": "jinja-html",
    //     //
    // },
    "workbench.colorTheme": "Material",
    "restructuredtext.linter.extraArgs": [
        "--ignore D002 D004",
        "--max-line-length 200"
    ],
    "remote.SSH.configFile": "C:\\Users\\tbaur\\.ssh\\config",
    "todo-tree.highlights.enabled": true,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "todo-tree.tree.showScanModeButton": false,
    "python.insidersChannel": "off",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": [
                    //following will be in italic (=FlottFlott)
                    "comment",
                    "entity.name.type.class", //class names
                    "keyword", //import, export, return…
                    "constant", //String, Number, Boolean…, this, super
                    "storage.modifier", //static keyword
                    "storage.type.class.js", //class keyword
                ],
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": [
                    //following will be excluded from italics (VSCode has some defaults for italics)
                    "invalid",
                    "keyword.operator",
                    "constant.numeric.css",
                    "keyword.other.unit.px.css",
                    "constant.numeric.decimal.js",
                    "constant.numeric.json"
                ],
                "settings": {
                    "fontStyle": ""
                }
            }
        ]
    }
}