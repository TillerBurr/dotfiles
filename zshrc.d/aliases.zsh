#! /bin/zsh

alias pip3='python3 -m pip --user'
alias fix-zsh-plugins="find ~/.oh-my-zsh/custom -type f -print0 | xargs -0 sed -i -e 's/\r$//'"
alias alf='autoload -Uz'
alias ..='cd ..'
alias ...='cd ../..'
alias http='http --style=solarized'
alias l='ls -l'
alias ll='ls -al'
alias ls='ls --color=auto -N'



